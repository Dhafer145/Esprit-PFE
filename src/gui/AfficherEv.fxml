      <Button fx:id="btnM" layoutX="256.0" layoutY="347.0" mnemonicParsing="false" onAction="#getModView" text="Modifier" />
      <Button fx:id="btnAct" layoutX="20.0" layoutY="347.0" mnemonicParsing="false" onAction="#loadTable" text="Actualiser" />
      <Button fx:id="btnAff" layoutX="330.0" layoutY="347.0" mnemonicParsing="false" onAction="#getConsView" text="Afficher" />
   </children>
</AnchorPane>
